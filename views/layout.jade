html 
    head
        title
            //tell it to pull the title from server.js title, the object literals i have
            =title
        link(href="/vendor/bootstrap/dist/css/bootstrap.min.css",rel="stylesheet") 
        // link(href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css", rel="stylesheet")
    body
        =tabs
        //loop over the tabs
        each tab in tabs
            ul.nav.nav-tabs
                li
                    a(href=tab.path)
                        =tab.title
        
    
        //dot means div tag in jade
        .container
            .well
                h1 
                    =title